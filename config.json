{
  "redis": {
    "cluster": false,
    "host": "127.0.0.1",
    "port": 6379,
    "db": 0,
    "pool": 10,
    "prepend": "beamengine",
    "packetTTL": 120000,
    "frameTTL": 10000,
    "closeTimeout": 2000
  },
  "app" : {
    "port": 3000
  },
  "testing": {
    "db": 15,
    "port": 8765
  },
  "jobs": {
    "jpeg": {
      "method": "GET",
      "pathPattern": "^.*\\.(jpg|jpeg)$",
      "queue": "media_workers",
      "function": "makeJPEG",
      "post": false
    },
    "fileStore": {
      "method": ["PUT", "POST"],
      "pathPattern": "^/beams.*",
      "statusCode": [200, 201],
      "queue": "file_workers",
      "function": "storeCopy",
      "post": true
    },
    "dataMissing": {
      "method": "GET",
      "pathPattern": "^.*(packet|frame)_.*$",
      "statusCode": 404,
      "queue": "file_workers",
      "function": "retrieveCopy",
      "post": true
    }
  }
}
